{
  "name": "@flynnt/create-odc-project",
  "version": "1.4.5",
  "description": "A Node command line tool for scaffolding common ODC projects",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "@flynnt/create-odc-project": "bin/create-odc-project.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onedesign/create-odc-project.git"
  },
  "homepage": "https://github.com/onedesign/create-odc-project.git#readme",
  "bugs": {
    "url": "https://github.com/onedesign/create-odc-project.git/issues"
  },
  "keywords": [
    "cli",
    "create-odc-project",
    "scaffold",
    "generator"
  ],
  "author": "Ted Flynn",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "11.0.0",
    "@types/inquirer": "9.0.3",
    "@types/listr": "0.14.4",
    "@types/ncp": "2.0.5",
    "@types/node": "18.14.6",
    "@typescript-eslint/parser": "5.54.1",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "prettier": "2.8.4",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "7.0.2",
    "tslib": "2.5.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@types/eslint": "8.21.1",
    "arg": "5.0.2",
    "chalk": "5.2.0",
    "execa": "7.0.0",
    "inquirer": "9.1.4",
    "listr": "0.14.3",
    "ncp": "2.0.0",
    "pkg-install": "1.0.0"
  }
}
